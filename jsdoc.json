{
  "tags": {
    "allowUnknownTags": ["category"]
  },
  "opts": {
    "template": "node_modules/better-docs",
    "recurse": true,
    "verbose": true,
    "destination": "./docs/"
  },
  "plugins": [
    "node_modules/better-docs/typedef-import",
    "node_modules/better-docs/category"
  ],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  }
}
